{
  "categories": [
    {
      "labels": [
        "feature"
      ],
      "title": "## \ud83d\ude80 Features"
    },
    {
      "labels": [
        "fix"
      ],
      "title": "## \ud83d\udc1b Fixes"
    },
    {
      "labels": [
        "test"
      ],
      "title": "## \ud83e\uddea Tests"
    },
    {
      "labels":[
        "refactoring",
        "refactor"
      ],
      "title": "## ðŸ”ƒ Refactoring"
    },
    {
      "labels":[
        "breaking",
        "break"
      ],
      "title": "## ðŸ™€ Breaking"
    },    
    {
        "title": "## ðŸ“¦ Dependencies",
        "labels": ["dependencies"]
    },
    {
      "empty_content": "- no matching PRs",
      "exclude_labels": [
        "no-magic"
      ],
      "exhaustive": true,
      "labels": [
        "test",
        "magic"
      ],
      "title": "## \ud83e\uddea Tests and some \ud83e\ude84 Magic"
    }
  ],
  "duplicate_filter": {
    "method": "match",
    "on_property": "title",
    "pattern": "\\[ABC-....\\]"
  },
  "empty_template": "- no changes",
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "ignore_labels": [
    "ignore"
  ],
  "label_extractor": [
    {
      "flags": "gu",
      "pattern": "(.) (.+)",
      "target": "$1"
    },
    {
      "method": "match",
      "on_property": "title",
      "pattern": "\\[Issue\\]"
    }
  ],
  "max_back_track_time_days": 365,
  "max_pull_requests": 200,
  "max_tags_to_fetch": 200,
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "sort": {
    "on_property": "mergedAt",
    "order": "ASC"
  },
  "tag_resolver": {
    "filter": {
      "flags": "gu",
      "pattern": "api-(.+)"
    },
    "method": "semver"
  },
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ]
}
